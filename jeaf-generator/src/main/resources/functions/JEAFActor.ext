extension java::GeneratorCommons;

cached boolean isJEAFActor(uml::Element element):
    element.isStereotypeApplied("JEAF Actor");
    
cached uml::Actor asActor(uml::Element element):
    (uml::Actor) element;
    
cached List[uml::Actor] getAssociatedActors(uml::Element element):
    element.getRelationships().typeSelect(uml::Dependency).source.select(e|e.isJEAFActor());
