<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="[%p] %m\n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="INFO">
			<AppenderRef ref="STDOUT"/>
		</Root>
		<Logger name="JEAF_DEFAULT_LOGGER" level="ERROR" />
		<Logger name="JEAF_GENERATOR" level="WARN" />
		<Logger name="org.openarchitectureware.workflow" level="WARN" />
		<Logger name="org.andromda.beautifier.plugin.ImportBeautifierMojo" level="WARN" />		
	</Loggers>
</Configuration>